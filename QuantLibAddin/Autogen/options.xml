<?xml version="1.0"?>
<!-- DOCTYPE functionlist SYSTEM "functionlist.dtd" -->
<functionlist>
<headeronly>false</headeronly>

<function>
<name>QL_BLACKSCHOLES</name>
<codename>qlBlackScholes</codename>
<desc>construct and return a handle to a Black Scholes object</desc>
<handle>true</handle>
<parameters>
	<param>
		<name>dividendYield</name>
		<type>double</type>
		<desc>dividend yield</desc>
	</param>
	<param>
		<name>riskFreeRate</name>
		<type>double</type>
		<desc>risk free rate</desc>
	</param>
	<param>
		<name>volatility</name>
		<type>double</type>
		<desc>volatility</desc>
	</param>
	<param>
		<name>underlying</name>
		<type>double</type>
		<desc>underlying</desc>
	</param>
	<param>
		<name>todaysDate</name>
		<type>long</type>
		<desc>today's date</desc>
	</param>
	<param>
		<name>settlementDate</name>
		<type>long</type>
		<desc>settlement date</desc>
	</param>
</parameters>
<returnval>
	<type>propertyvector</type>
	<desc>vector of properties describing Black Scholes object</desc>
</returnval>
</function>

<function>
<name>QL_OPTION</name>
<codename>qlOption</codename>
<desc>construct and return a handle to an Option object</desc>
<handle>true</handle>
<parameters>
	<param>
		<name>handleStochastic</name>
		<type>string</type>
		<desc>handle of the Black Scholes object</desc>
	</param>
	<param>
		<name>typeOption</name>
		<type>string</type>
		<desc>option type</desc>
	</param>
	<param>
		<name>strike</name>
		<type>double</type>
		<desc>strike</desc>
	</param>
	<param>
		<name>timeSteps</name>
		<type>long</type>
		<desc>time steps</desc>
	</param>
	<param>
		<name>exerciseDate</name>
		<type>long</type>
		<desc>exercise date</desc>
	</param>
	<param>
		<name>settlementDate</name>
		<type>long</type>
		<desc>settlement date</desc>
	</param>
</parameters>
<returnval>
	<type>propertyvector</type>
	<desc>vector of properties describing Option</desc>
</returnval>
</function>

<function>
<name>QL_OPTION_SETENGINE</name>
<codename>qlOptionSetEngine</codename>
<desc>change the pricing engine of an existing Option object</desc>
<handle>false</handle>
<parameters>
	<param>
		<name>handle</name>
		<type>string</type>
		<desc>handle of the Option object</desc>
	</param>
	<param>
		<name>engineName</name>
		<type>string</type>
		<desc>engine name</desc>
	</param>
	<param>
		<name>timeSteps</name>
		<type>long</type>
		<desc>time steps</desc>
	</param>
</parameters>
<returnval>
	<type>propertyvector</type>
	<desc>vector of properties reflecting revised Option</desc>
</returnval>
</function>

</functionlist>
