<function>
<name>QL_STOCHASTIC_PROCESS</name>
<codename>qlStochasticProcess</codename>
<desc>construct and return a handle to a Stochastic Process object</desc>
<qlfunction>StochasticProcess</qlfunction>
<constructor>true</constructor>
<parameters>
	<param>
		<name>underlying</name>
		<type>double</type>
		<tensorrank>scalar</tensorrank>
		<desc>underlying</desc>
	</param>
	<param>
		<name>dayCounterID</name>
		<type>string</type>
		<tensorrank>scalar</tensorrank>
		<desc>ID of day counter</desc>
	</param>
	<param>
		<name>settlementDate</name>
		<type>long</type>
		<tensorrank>scalar</tensorrank>
		<desc>settlement date</desc>
	</param>
	<param>
		<name>riskFreeRate</name>
		<type>double</type>
		<tensorrank>scalar</tensorrank>
		<desc>risk free rate</desc>
	</param>
	<param>
		<name>dividendYield</name>
		<type>double</type>
		<tensorrank>scalar</tensorrank>
		<desc>dividend yield</desc>
	</param>
	<param>
		<name>volatility</name>
		<type>double</type>
		<tensorrank>scalar</tensorrank>
		<desc>volatility</desc>
	</param>
</parameters>
<returnval>
	<type>propertyvector</type>
	<desc>vector of properties describing Stochastic Process object</desc>
</returnval>
</function>

