<?xml version="1.0"?>
<!-- DOCTYPE functionlist SYSTEM "functionlist.dtd" -->
<functionlist>
<headeronly>true</headeronly>

<function>
<name>QL_QUERY</name>
<codename>qlQuery</codename>
<desc>retrieve the properties of a given object</desc>
<qlfunction></qlfunction>
<constructor>false</constructor>
<parameters>
	<param>
		<name>handleObject</name>
		<type>string</type>
		<desc>handle of object to be queried</desc>
	</param>
</parameters>
<returnval>
	<type>propertyvector</type>
	<desc>vector of properties describing queried object</desc>
</returnval>
</function>

<function>
<name>QL_LOGFILE</name>
<codename>qlLogfile</codename>
<qlfunction></qlfunction>
<desc>begin logging to named file</desc>
<constructor>false</constructor>
<parameters>
	<param>
		<name>logFileName</name>
		<type>string</type>
		<desc>path and name of log file</desc>
	</param>
</parameters>
<returnval>
	<type>string</type>
	<desc>name of logfile</desc>
</returnval>
</function>

</functionlist>
