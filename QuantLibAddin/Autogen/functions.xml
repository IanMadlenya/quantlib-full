<?xml version="1.0"?>
<!-- DOCTYPE functionlist SYSTEM "functionlist.dtd" -->
<functionlist>
<headeronly>true</headeronly>

<function>
<name>QL_QUERY</name>
<codename>qlQuery</codename>
<description>retrieve the properties of a given object</description>
<handle>false</handle>
<parameters>
	<param>
		<name>handleObject</name>
		<type>string</type>
		<desc>handle of object to be queried</desc>
	</param>
</parameters>
</function>

<function>
<name>QL_LOGFILE</name>
<codename>qlLogfile</codename>
<description>begin logging to named file</description>
<handle>false</handle>
<parameters>
	<param>
		<name>logFileName</name>
		<type>string</type>
		<desc>path and name of log file</desc>
	</param>
</parameters>
</function>

</functionlist>
